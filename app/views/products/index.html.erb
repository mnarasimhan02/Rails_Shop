<% cache(cache_key_for_products) do %>
<p id="notice"><%= notice %></p>
<h1>Listing products</h1>
<table>
  <tbody>
  <div class="container-fluid">
      <div class="row">
       <% @products.each do |product| %>
           <%= render product %>
       <% end %>
       </div>
  </div>
  </tbody>
</table>
<br><br>
<% end %>
<%= link_to 'New Product', new_product_path %>
